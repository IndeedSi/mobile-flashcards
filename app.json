{
  "expo": {
    "name": "UdaciCards",
    "description": "An app to help you manage flash cards and take quiz on the cards",
    "slug": "mobile-flashcards",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "githubUrl": "https://github.com/IndeedSi/mobile-flashcards",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.udacicards.exp"
    },
    "android": {
      "package": "org.udacicards.exp"
    }
  }
}